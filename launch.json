{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run echo",
      "name": "npm",
      "request": "launch",
      "type": "node-terminal",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    },
    {
      "name": "Csharp",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "cmd",
      "runtimeArgs": [ "/c", "csc /t:winexe ${file} && ${fileBasenameNoExtension}.exe && shift_f5.exe" ],
      "console": "integratedTerminal"
    }
  ]
}